-- ======================================================
-- SERVICES / THEME / GEOMETRY
-- ======================================================
local TweenService = game:GetService("TweenService")
local UIS = game:GetService("UserInputService")
local RS = game:GetService("RunService")
local Player = game.Players.LocalPlayer
local Mouse = Player:GetMouse()
local HeaderSize = 30
local Red   = Color3.new(0.9,0,0)
local Green = Color3.new(0.26,0.85,0)
local Grey  = Color3.new(0.25,0.25,0.25)
local Blue  = Color3.new(0.26,0.53,0.95)

local PAGE_X, PAGE_Y, PAGE_W, PAGE_H = 60, 60, 380, 210
local MARGIN, ROW_H = 6, 36
local TOP_PADDING = MARGIN + ROW_H + MARGIN + ROW_H + MARGIN  -- 90

-- connection/tween tracking for clean teardown
local Connections, GlowTweens = {}, {}
local function track(conn) table.insert(Connections, conn); return conn end
local function trackTween(tw) table.insert(GlowTweens, tw); return tw end

-- ======================================================
-- ROOT GUI + BACKGROUND + GLOW
-- ======================================================
local freemotegui = Instance.new("ScreenGui")
freemotegui.Name = getgenv().freeEmotesName or "FREEMOTES by @lulaslollipop"
freemotegui.ResetOnSpawn = false
freemotegui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
freemotegui.Parent = Player:WaitForChild("PlayerGui")

local PageBG = Instance.new("Frame", freemotegui)
PageBG.Name = "PageBG"
PageBG.Size = UDim2.new(0, PAGE_W, 0, PAGE_H)
PageBG.Position = UDim2.new(0, PAGE_X, 0, PAGE_Y)
PageBG.BackgroundColor3 = Color3.fromRGB(48,48,48)
PageBG.BorderSizePixel = 0
PageBG.ZIndex = 0
Instance.new("UICorner", PageBG)

local function addGlow(target)
    local stroke = Instance.new("UIStroke")
    stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    stroke.Color = Color3.fromRGB(255, 40, 40)
    stroke.Thickness = 2
    stroke.Transparency = 0.2
    stroke.Parent = target

    local tw = TweenService:Create(stroke, TweenInfo.new(0.9, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut, -1, true), {
        Thickness = 4, Transparency = 0.05
    })
    tw:Play()
    trackTween(tw)
    return stroke
end

addGlow(PageBG) -- glow for pages 1‚Äì2

local function CreateScrollingFrame(name)
    local sf = Instance.new("ScrollingFrame")
    sf.Name = name
    sf.Size = UDim2.new(0, PAGE_W, 0, PAGE_H - TOP_PADDING)
    sf.Position = UDim2.new(0, PAGE_X, 0, PAGE_Y + TOP_PADDING)
    sf.BackgroundTransparency = 1
    sf.BorderSizePixel = 0
    sf.CanvasSize = UDim2.new(0,0,0,0)
    sf.ScrollBarThickness = 6
    sf.ZIndex = 1
    sf.Parent = freemotegui
    return sf
end

local AnimationsFrame = CreateScrollingFrame("AnimationsFrame")
local StolenAnimationsFrame = CreateScrollingFrame("StolenAnimationsFrame")
StolenAnimationsFrame.Visible = false

