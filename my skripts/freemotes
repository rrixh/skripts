end))
]]--
end

-- ======================================================
-- klose: hard teardown (stop, kill highlight, kancel tweens, disconnect all)
-- ======================================================
local function tearDownAndClose()
    StopAllAnimations()
    setStealMode(false)
    if pickClickConn then pickClickConn:Disconnect(); pickClickConn = nil end
    if dragInputConn then dragInputConn:Disconnect(); dragInputConn = nil end
    for _, tw in ipairs(GlowTweens) do pcall(function() tw:Cancel() end) end
    for _, c in ipairs(Connections) do pcall(function() c:Disconnect() end) end
    table.clear(Connections); table.clear(GlowTweens)
    _G.__setStealMode = nil
    freemotegui:Destroy()
end
track(CloseButton.MouseButton1Click:Connect(tearDownAndClose))

-- ======================================================
-- INIT
-- ======================================================
local function filterInit() end
filterInit()

showPage(1)
warn("Number of emotes:", math.floor(#animationz/2))

end); -- pkall
if not hub then rnote(tostring(no)) warn(no) return end;