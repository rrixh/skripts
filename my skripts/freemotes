-- FREEMOTES HUB v3.2 (freakymotes)--
local hub,no=pcall(function()

local lastupdate = "Sep 29, "..os.date("%Y")

local loading = 0;
local rnote,RRixh,n,fonts,add;

local rnote = function(txt, dur)
RRixh=loadstring(game:HttpGet("https://raw.githubusercontent.com/rrixh/uwuware/main/skripts/AkaliNotif", true))();n = RRixh.Notify; n({Description = txt,Duration=dur or 7}); 
end;

--// get ani/emo name by id \\--
getgenv().animKaxhe = {};
getgenv().getname = function(assetId, mode)
if (not mode) or (not tonumber(mode)) then mode = 2; end;
local mps = cloneref(game:GetService("MarketplaceService"))
    local animId = tostring(assetId)
  if not animId:match("%d+") then
        return "[Invalid ID]"
    end;
  animId = animId:match("%d+");
if (animKaxhe[animId] and animKaxhe[animId][mode]) then
   return animKaxhe[animId][mode]
 end;
    local result = "[Unknown]"
    if mode == 1 then
        local anim = Instance.new("Animation")
        anim.AnimationId = "rbxassetid://"..animId
        result = anim.Name ~= "" and anim.Name or "Animation"
        anim:Destroy()
 elseif (mode == 2) or (mode==nil) or (mode:match("^%s*$"))then
        pcall(function()
            local info = mps:GetProductInfo(tonumber(animId))
            result = info.Name or "[Unknown]"
        end)
    end;
    animKaxhe[animId] = animKaxhe[animId] or {}
    animKaxhe[animId][mode] = result
    return result
end;

rnote("FREEMOTES V3 waiting for game to load...", 4)

repeat loading = loading + 2;
 print("loading freemotes (FREEakymotes)...", loading, "sekond(s)")
until getname and rnote and game:IsLoaded();

-- ======= FONTS =========--
