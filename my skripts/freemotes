title = "Bangers", -- GothamBold, Bangers
buttons = "Highway", -- Gotham
emotes = "DenkOne", -- GothamSemibold, Highway
pages = "FredokaOne",
emotekount = "FredokaOne",
kreator = "DenkOne",
lastupdated = "DenkOne", --GothamSemibold
updates = "Bodoni",
minimizeAndKlose = "FredokaOne", -- GothamBold
arrows = "FredokaOne", -- Gotham
}

fonts = getgenv().fonts;
local function F(name)
    return Enum.Font[name] or Enum.Font.Gotham
end;

