-- ======================================================
-- PAGE STATE / VISIBILITY / MINIMIZE
-- ======================================================
local currentPage, minimized = 1, false

local function refreshInfoEmoteCount()
    InfoEmoteCount.Text = "Emotes: " .. tostring(math.floor(#animationz/2))
end

local function applyVisibility()
    if minimized then
        AnimationsFrame.Visible = false
        StolenAnimationsFrame.Visible = false
        InfoFrame.Visible = false
        Overlay1.Visible = false
        Overlay2.Visible = false
        PageBG.Visible = false
    else
        AnimationsFrame.Visible = (currentPage == 1)
        StolenAnimationsFrame.Visible = (currentPage == 2)
        InfoFrame.Visible = (currentPage == 3)
        Overlay1.Visible = (currentPage == 1)
        Overlay2.Visible = (currentPage == 2)
        PageBG.Visible = (currentPage == 1 or currentPage == 2)
    end
end

local function showPage(page)
    currentPage = page
    PageBadge.Text = "Page: " .. tostring(page)
    if page == 3 then
        refreshInfoEmoteCount()
  InfoDate.Text = "Date updated: "..lastupdate
    end
    applyVisibility()
end

track(PrevBtn.MouseButton1Click:Connect(function()
    if currentPage > 1 then showPage(currentPage - 1) end
end))
track(NextBtn.MouseButton1Click:Connect(function()
    if currentPage < 3 then showPage(currentPage + 1) end
end))

track(MinimizeButton.MouseButton1Click:Connect(function()
    minimized = not minimized
    if _G.__setStealMode then _G.__setStealMode(false) end
    applyVisibility()
end))

-- ======================================================
-- DRAGGING (moves all)
-- ======================================================
local dragging, dragOffset = false, Vector2.new(0,0)
track(Header.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        local pos = input.Position
        dragOffset = Vector2.new(pos.X - PageBG.AbsolutePosition.X, pos.Y - PageBG.AbsolutePosition.Y)
    end
end))
track(Header.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = false
    end
end))
local dragInputConn
dragInputConn = track(UIS.InputChanged:Connect(function(input)
    if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
        local pos = input.Position
        local newX = pos.X - dragOffset.X
        local newY = pos.Y - dragOffset.Y
        local scPos  = UDim2.new(0,newX,0,newY + TOP_PADDING)
        local basePos= UDim2.new(0,newX,0,newY)

        PageBG.Position            = basePos
        AnimationsFrame.Position   = scPos
        StolenAnimationsFrame.Position = scPos
        InfoFrame.Position         = basePos
        Overlay1.Position          = basePos
        Overlay2.Position          = basePos
        Header.Position            = basePos + UDim2.new(0,0,0,-HeaderSize)
    end
end))

-- ======================================================
-- LIST LAYOUTS
-- ======================================================
local function AutoCanvas(scroller)
    local layout = Instance.new("UIListLayout", scroller)
    layout.SortOrder = Enum.SortOrder.LayoutOrder
    layout.Padding = UDim.new(0,6)
    track(layout:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
        scroller.CanvasSize = UDim2.new(0,0,0,layout.AbsoluteContentSize.Y + 12)
    end))
    return layout
end
AutoCanvas(AnimationsFrame)
AutoCanvas(StolenAnimationsFrame)

-- ======================================================
-- PLAY/STOP HELPERS
-- ======================================================
local Active = {track = nil, id = nil, btn = nil}
local function buildAnimationId(raw)
    local s = tostring(raw)
    if string.find(s, "rbxassetid://") then return s end
    local digits = s:match("%d+")
    if digits then return "rbxassetid://" .. digits end
    return "rbxassetid://" .. s
end

local function StopAllAnimations()
    local char = Player.Character
    local hum = char and char:FindFirstChildOfClass("Humanoid")
    if hum then
        for _, tr in ipairs(hum:GetPlayingAnimationTracks()) do
            pcall(function() tr:Stop() end)
        end
    end
    if Active.btn and Active.btn.Parent then Active.btn.BackgroundColor3 = Red end
    Active.track, Active.id, Active.btn = nil, nil, nil
end

-- ======================================================
-- CreateEmoteEntry
-- ======================================================
local function CreateEmoteEntry(name, id, parent)
    local entry = Instance.new("Frame")
    entry.Size = UDim2.new(1, 0, 0, 40)
    entry.BackgroundTransparency = 1
    entry.Name = "EmoteEntry"
    entry.Parent = parent

    local hLayout = Instance.new("UIListLayout", entry)
    hLayout.FillDirection = Enum.FillDirection.Horizontal
    hLayout.HorizontalAlignment = Enum.HorizontalAlignment.Left
    hLayout.SortOrder = Enum.SortOrder.LayoutOrder
    hLayout.Padding = UDim.new(0,6)

    local btn = Instance.new("TextButton", entry)
    btn.Size = UDim2.new(0.68, 0, 1, 0)
    btn.BackgroundColor3 = Red
    btn.Text = name
    btn.Name = "EmoteButton"
    btn.Font = F(fonts.emotes)
    btn.TextScaled = true
    btn.TextColor3 = Color3.new(1,1,1)
    btn:SetAttribute("AnimId", id)
    Instance.new("UICorner", btn)

    local speedBox = Instance.new("TextBox", entry)
    speedBox.Size = UDim2.new(0.32, 0, 1, 0)
    speedBox.PlaceholderText = "speed (1)"
    speedBox.Text = ""
    speedBox.Font = F(fonts.buttons)
    speedBox.TextScaled = true
    speedBox.BackgroundColor3 = Color3.fromRGB(60,60,60)
    speedBox.TextColor3 = Color3.new(1,1,1)
    speedBox.Name = "SpeedBox"
    Instance.new("UICorner", speedBox)

    track(btn.MouseButton1Click:Connect(function()
        local animIdStr = buildAnimationId(id)

        if Active.id == tostring(id) then
            if Active.track then pcall(function() Active.track:Stop() end) end
            btn.BackgroundColor3 = Red
            Active.track, Active.id, Active.btn = nil, nil, nil
            return
        end

        if Active.track then
            pcall(function() Active.track:Stop() end)
            if Active.btn and Active.btn.Parent then Active.btn.BackgroundColor3 = Red end
        end

        local char = Player.Character
        local humanoid = char and char:FindFirstChildOfClass("Humanoid")
        if not humanoid then warn("No humanoid; can't play anim") return end

        local animObj = Instance.new("Animation")
        animObj.AnimationId = animIdStr
        local ok, trackAnim = pcall(function() return humanoid:LoadAnimation(animObj) end)
        if not ok or not trackAnim then warn("Failed to load anim:", animIdStr) return end

        trackAnim.Looped = true
        trackAnim.Priority = Enum.AnimationPriority.Action
        trackAnim:Play()

        local sp = tonumber(speedBox.Text)
        if sp and trackAnim.AdjustSpeed then pcall(function() trackAnim:AdjustSpeed(sp) end) end

        Active.track, Active.id, Active.btn = trackAnim, tostring(id), btn
        btn.BackgroundColor3 = Green
    end))

    track(speedBox.FocusLost:Connect(function()
        local sp = tonumber(speedBox.Text)
        if sp and Active and Active.track and Active.id == tostring(id) then
            pcall(function() Active.track:AdjustSpeed(sp) end)
        end
    end))

    return entry
end

-- Populate Page 1
for i = 1, #animationz, 2 do
    local id = animationz[i]
    local name = tostring(animationz[i+1] or "Animation")
    CreateEmoteEntry(name, id, AnimationsFrame)
end

-- ======================================================
-- Search factory (with ‚ùå)
-- ======================================================
local function CreateInteractable(size, name, placeholder, bgColor, cornerRadiusPx, withStroke, kind, parent)
    local container = Instance.new("Frame")
    container.Name = name .. "Container"
    container.Size = size
    container.BackgroundTransparency = 1
    container.Parent = parent
    container.ZIndex = parent.ZIndex

    local h = Instance.new("UIListLayout", container)
    h.FillDirection = Enum.FillDirection.Horizontal
    h.Padding = UDim.new(0,6)

    if kind == "TextBox" then
        local box = Instance.new("TextBox")
        box.Name = "SearchbarBox"
        box.Size = UDim2.new(1, -34, 1, 0)
        box.BackgroundColor3 = bgColor or Grey
        box.TextColor3 = Color3.new(1,1,1)
        box.PlaceholderText = placeholder or ""
        box.ClearTextOnFocus = false
        box.Text = ""
        box.Font = F(fonts.buttons)
        box.TextScaled = true
        box.ZIndex = parent.ZIndex

        local radius = tonumber(cornerRadiusPx) or 6
        local corner = Instance.new("UICorner", box)
        corner.CornerRadius = UDim.new(0, radius)

        local clear = Instance.new("TextButton")
        clear.Name = "ClearSearch"
        clear.Size = UDim2.new(0, 28, 1, 0)
        clear.BackgroundColor3 = Color3.fromRGB(70,70,70)
        clear.Text = "‚ùå"
        clear.Font = F(fonts.buttons)
        clear.TextScaled = true
        clear.ZIndex = parent.ZIndex
        Instance.new("UICorner", clear)

        box.Parent = container
        clear.Parent = container

        track(clear.MouseButton1Click:Connect(function()
            box.Text = ""
            box:ReleaseFocus()
        end))

        return box
    end

    return container
end

local function filterList(scroller, queryLower)
    for _, child in ipairs(scroller:GetChildren()) do
        if child:IsA("Frame") and child.Name == "EmoteEntry" then
            local btn = child:FindFirstChild("EmoteButton")
            local txt = btn and (btn.Text or "") or ""
            child.Visible = (queryLower == "") or (string.find(string.lower(txt), queryLower, 1, true) ~= nil)
        end
    end
end

-- ======================================================
-- PAGE 1 OVERLAY
-- ======================================================
do
    local row1 = Instance.new("Frame", Overlay1)
    row1.Size = UDim2.new(1, -2*MARGIN, 0, ROW_H)
    row1.Position = UDim2.new(0, MARGIN, 0, MARGIN)
    row1.BackgroundTransparency = 1
    row1.ZIndex = 4

    local StopBtn = Instance.new("TextButton", row1)
    StopBtn.Name = "StopAllBtn_P1"
    StopBtn.Size = UDim2.new(1, 0, 1, 0)
    StopBtn.Text = "üö® STOP ANIMATIONS"
    StopBtn.Font = F(fonts.buttons)
    StopBtn.TextScaled = true
    StopBtn.BackgroundColor3 = Red
    StopBtn.TextColor3 = Color3.new(1,1,1)
    StopBtn.ZIndex = 4
    Instance.new("UICorner", StopBtn)
    track(StopBtn.MouseButton1Click:Connect(StopAllAnimations))

    local row2 = Instance.new("Frame", Overlay1)
    row2.Size = UDim2.new(1, -2*MARGIN, 0, ROW_H)
    row2.Position = UDim2.new(0, MARGIN, 0, MARGIN*2 + ROW_H)
    row2.BackgroundTransparency = 1
    row2.ZIndex = 4

    SearchBox_P1 = CreateInteractable(UDim2.new(1,0,1,0), "Searchbar", "search animations here...", Grey, 6, false, "TextBox", row2)
    track(SearchBox_P1:GetPropertyChangedSignal("Text"):Connect(function()
        filterList(AnimationsFrame, string.lower(SearchBox_P1.Text or ""))
    end))
    filterList(AnimationsFrame, "")
end;

-- ======================================================
-- STEAL MODE (Page 2) with proper highlight teardown
-- ======================================================
local StealMode, highlightObj, stealConn = false, nil, nil
local pickClickConn

local function destroyHighlight()
    if highlightObj then
        highlightObj:Destroy()
        highlightObj = nil
    end
end

local function setStealMode(on)
    if on then
        StealMode = true
        destroyHighlight()
        highlightObj = Instance.new("Highlight")
        highlightObj.FillColor = Blue
        highlightObj.OutlineTransparency = 1
        highlightObj.Parent = workspace

        if stealConn then stealConn:Disconnect() end
        stealConn = track(RS.Heartbeat:Connect(function()
            if not StealMode then return end
            local target = Mouse.Target
            if target then
                local model = target:FindFirstAncestorWhichIsA("Model")
                if model and model:FindFirstChildOfClass("Humanoid") then
                    highlightObj.Adornee = model
                else
                    highlightObj.Adornee = nil
                end
            else
                highlightObj.Adornee = nil
            end
        end))
    else
        StealMode = false
        if stealConn then stealConn:Disconnect(); stealConn = nil end
        destroyHighlight()
    end
end
_G.__setStealMode = setStealMode

-- >>> FIXED: always pass the REAL ID into CreateEmoteEntry <<<
local function AddStolenEntry(label, animId)
    CreateEmoteEntry(label, animId, StolenAnimationsFrame)
    refreshInfoEmoteCount()
end

do
    local row1 = Instance.new("Frame", Overlay2)
    row1.Size = UDim2.new(1, -2*MARGIN, 0, ROW_H)
    row1.Position = UDim2.new(0, MARGIN, 0, MARGIN)
    row1.BackgroundTransparency = 1
    row1.ZIndex = 4
    local h1 = Instance.new("UIListLayout", row1)
    h1.FillDirection = Enum.FillDirection.Horizontal
    h1.Padding = UDim.new(0,6)

    local StopBtn2 = Instance.new("TextButton", row1)
    StopBtn2.Name = "StopAllBtn_P2"
    StopBtn2.Size = UDim2.new(0.5, -3, 1, 0)
    StopBtn2.Text = "üö® STOP ANIMATIONS"
    StopBtn2.Font = F(fonts.buttons)
    StopBtn2.TextScaled = true
    StopBtn2.BackgroundColor3 = Red
    StopBtn2.TextColor3 = Color3.new(1,1,1)
    StopBtn2.ZIndex = 4
    Instance.new("UICorner", StopBtn2)
    track(StopBtn2.MouseButton1Click:Connect(function()
        StopAllAnimations()
        setStealMode(false)
    end))

    local SearchHalf = Instance.new("Frame", row1)
    SearchHalf.Size = UDim2.new(0.5, -3, 1, 0)
    SearchHalf.BackgroundTransparency = 1
    SearchHalf.ZIndex = 4

    local SearchBox_P2 = CreateInteractable(UDim2.new(1,0,1,0), "Searchbar", "search stolen animations...", Grey, 6, false, "TextBox", SearchHalf)
    track(SearchBox_P2:GetPropertyChangedSignal("Text"):Connect(function()
        filterList(StolenAnimationsFrame, string.lower(SearchBox_P2.Text or ""))
    end))
    filterList(StolenAnimationsFrame, "")

    local row2 = Instance.new("Frame", Overlay2)
    row2.Size = UDim2.new(1, -2*MARGIN, 0, ROW_H)
    row2.Position = UDim2.new(0, MARGIN, 0, MARGIN*2 + ROW_H)
    row2.BackgroundTransparency = 1
    row2.ZIndex = 4
    local h2 = Instance.new("UIListLayout", row2)
    h2.FillDirection = Enum.FillDirection.Horizontal
    h2.Padding = UDim.new(0,6)

    local StealPlayBtn = Instance.new("TextButton", row2)
    StealPlayBtn.Size = UDim2.new(0.5, -3, 1, 0)
    StealPlayBtn.Text = "Steal Play"
    StealPlayBtn.Font = F(fonts.buttons)
    StealPlayBtn.TextScaled = true
    StealPlayBtn.BackgroundColor3 = Grey
    StealPlayBtn.TextColor3 = Color3.new(1,1,1)
    StealPlayBtn.ZIndex = 4
    Instance.new("UICorner", StealPlayBtn)

    local PickStealBtn = Instance.new("TextButton", row2)
    PickStealBtn.Size = UDim2.new(0.5, -3, 1, 0)
    PickStealBtn.Text = "Pick & Steal"
    PickStealBtn.Font = F(fonts.buttons)
    PickStealBtn.TextScaled = true
    PickStealBtn.BackgroundColor3 = Grey
    PickStealBtn.TextColor3 = Color3.new(1,1,1)
    PickStealBtn.ZIndex = 4
    Instance.new("UICorner", PickStealBtn)

    -- >>> FIXED: pass the actual ID (Active.id), not getname(Active.id)
    track(StealPlayBtn.MouseButton1Click:Connect(function()
        if Active.id then
local animName = tostring(getname(Active.id));
local animId = tonumber(Active.id);
    local label = "Stole: "..animName.." ["..animId.."]"
     AddStolenEntry(label, Active.id)
  warn("stolen animation ID:", Active.id)
        else
 warn("no aktive animation to steal")
        end
        setStealMode(false)
PickStealBtn.BackgroundColor3 = Grey
    end))

    track(PickStealBtn.MouseButton1Click:Connect(function()
        setStealMode(not StealMode)
        PickStealBtn.BackgroundColor3 = StealMode and Blue or Grey
    end))

    pickClickConn = track(UIS.InputBegan:Connect(function(input, processed)
        if not StealMode or processed then return end
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            local target = Mouse.Target
            if not target then return end
            local model = target:FindFirstAncestorWhichIsA("Model")
            if not model then return end
            local humanoid = model:FindFirstChildOfClass("Humanoid")
            if not humanoid then return end

            for _, theirTrack in ipairs(humanoid:GetPlayingAnimationTracks()) do
                local animIdRaw = tostring(theirTrack.Animation and theirTrack.Animation.AnimationId or "")
                local digits = animIdRaw:match("%d+")
                local finalId = digits or animIdRaw
  if finalId and finalId ~= "" then
local idname = tostring(getname(finalId));
local idnumber = tonumber(finalId);
     local label = "Stole: ".. idname.." ["..idnumber.."]"
                    AddStolenEntry(label, finalId) -- >>> pass ID
                    setStealMode(false)
                    PickStealBtn.BackgroundColor3 = Grey
                    warn("Stolen animation id from player:", finalId)
                    break
                end
            end
        end
    end))

